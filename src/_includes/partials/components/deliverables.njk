{% set deliverableStory = collections.deliverable %}
{% if deliverableStory.length %}
<section id="wrapper spaced">
<section id="case-study-caro" class="case-column-version spaced-90 col-cream">
    <div class="case-study-left">
        <div class="heading-inline-wrapper">
            <h3>{{ pageSection.deliverableGalleryTitle }}</h3>
            <p>{{ pageSection.deliverableDescription }}</p>
        </div>
    </div>
    <div class="case-study-right">
        <div class="main-carousel case-caro-content shorter-caro" data-flickity='{ "wrapAround": true, "cellAlign": "left", "contain": true }'>
            {% for story in deliverableStory %}
                <div class="quote-cell carousel-cell col-yellow">
                    <div class="quote-cell-top">
                        <b>{{ story.data.deliverableOutcomeTitle | safe }}</b>
                        <p>{{ story.data.deliverableOutcome | safe }}</p>
                    </div>
                    <div class="quote-cell-bot">
                        <b>{{ story.data.deliverableOutputsTitle | safe }}</b> 
                        <p>{{ story.data.deliverableOutputs | safe }}</p> 
                        </br>
                        <b>{{ story.data.deliverableSkillsTitle | safe }}</b>
                        <p>{{ story.data.deliverableSkills | safe }}</p>
                    </div>
                </div>

            {% endfor %}
        </div>
    </div>
</section>
</section>
{% endif %}